{
  "dashboard": {
    "title": "Soba DEX - ML & Analytics Dashboard (v3.4.0)",
    "tags": ["soba", "ml", "analytics", "ai"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "ML Anomaly Detection",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(soba_dex_ml_anomalies_detected_total[5m])",
            "legendFormat": "Anomalies Detected ({{type}})"
          },
          {
            "expr": "histogram_quantile(0.95, rate(soba_dex_ml_processing_seconds_bucket{operation=\"anomaly_detection\"}[5m]))",
            "legendFormat": "Detection Latency (P95)"
          }
        ]
      },
      {
        "id": 2,
        "title": "ML Model Performance",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "soba_dex_ml_anomalies_detected_total",
            "legendFormat": "Total Anomalies"
          }
        ]
      },
      {
        "id": 3,
        "title": "ML Processing Time",
        "type": "heatmap",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "rate(soba_dex_ml_processing_seconds_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Predictive Scaling",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "soba_dex_scaling_instance_count",
            "legendFormat": "Current Instances"
          },
          {
            "expr": "rate(soba_dex_scaling_actions_total{action=\"scale_up\"}[5m])",
            "legendFormat": "Scale Up Actions"
          },
          {
            "expr": "rate(soba_dex_scaling_actions_total{action=\"scale_down\"}[5m])",
            "legendFormat": "Scale Down Actions"
          }
        ]
      },
      {
        "id": 5,
        "title": "Scaling Prediction Accuracy",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(soba_dex_scaling_prediction_seconds_bucket[5m]))",
            "legendFormat": "Prediction Time (P95)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            },
            "min": 0,
            "max": 2,
            "unit": "s"
          }
        }
      },
      {
        "id": 6,
        "title": "A/B Testing Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "soba_dex_ab_tests_active",
            "legendFormat": "Active Tests"
          },
          {
            "expr": "rate(soba_dex_ab_test_assignments_total[5m])",
            "legendFormat": "Assignments/sec"
          }
        ]
      },
      {
        "id": 7,
        "title": "A/B Test Conversions",
        "type": "graph",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 12},
        "targets": [
          {
            "expr": "rate(soba_dex_ab_test_conversions_total[5m])",
            "legendFormat": "{{experiment}} - {{variant}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "User Behavior Analytics",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(soba_dex_user_events_tracked_total[5m])",
            "legendFormat": "Events/sec ({{event}})"
          },
          {
            "expr": "soba_dex_user_sessions_active",
            "legendFormat": "Active Sessions"
          }
        ]
      },
      {
        "id": 9,
        "title": "Churn Prediction Distribution",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(soba_dex_user_churn_score_bucket[5m])",
            "legendFormat": "{{risk}} - {{le}}"
          }
        ]
      },
      {
        "id": 10,
        "title": "Auto-tuning Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "soba_dex_auto_tuning_performance_score",
            "legendFormat": "Performance Score"
          },
          {
            "expr": "rate(soba_dex_auto_tuning_changes_applied_total{result=\"success\"}[5m])",
            "legendFormat": "Successful Changes"
          },
          {
            "expr": "rate(soba_dex_auto_tuning_changes_applied_total{result=\"failed\"}[5m])",
            "legendFormat": "Failed Changes"
          }
        ]
      },
      {
        "id": 11,
        "title": "ML Model Training Time",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(soba_dex_ml_model_training_seconds_bucket[5m]))",
            "legendFormat": "Training Time (P95)"
          }
        ]
      },
      {
        "id": 12,
        "title": "Auto-tuning Analysis Time",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(soba_dex_auto_tuning_analysis_seconds_bucket[5m]))",
            "legendFormat": "Analysis Time (P95)"
          },
          {
            "expr": "histogram_quantile(0.99, rate(soba_dex_auto_tuning_analysis_seconds_bucket[5m]))",
            "legendFormat": "Analysis Time (P99)"
          }
        ]
      },
      {
        "id": 13,
        "title": "ML Service Health",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "rate(soba_dex_ml_anomalies_detected_total[5m])",
            "format": "table",
            "instant": true
          },
          {
            "expr": "soba_dex_scaling_instance_count",
            "format": "table",
            "instant": true
          },
          {
            "expr": "soba_dex_ab_tests_active",
            "format": "table",
            "instant": true
          },
          {
            "expr": "soba_dex_user_sessions_active",
            "format": "table",
            "instant": true
          },
          {
            "expr": "soba_dex_auto_tuning_performance_score",
            "format": "table",
            "instant": true
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
